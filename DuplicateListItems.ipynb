    "list1 = [1, 2, 3, 2, 2, 3, 4]\n",
    "_size=len(list1);\n",
    "repeated = []\n",
    "for i in range(_size):\n",
    "    k=i+1;\n",
    "    for j in range(k,_size):\n",
    "        if list1[i] == list1[j] and list1[i] not in repeated:\n",
    "            repeated.append(list1[i])\n",
    "            \n",
    "\n",
    "print (repeated);\n"
